#map
  position absolute
  top 0
  left 0
  width 2002px
  height 1500px
  transform translate3d(0,0,0)
  overflow hidden
  perspective 400

  &.eastside
    background url(../images/Map2.png) no-repeat
  &.westside
    background url(../images/westside/west-side.svg) no-repeat

#drawCanvas
  position absolute
  top 0
  left 0
  width 5769px
  height 4102px

#snowCanvas
  position absolute
  top 0
  left 0
  width 100%
  height 100%
  pointer-events none

.obstruction
  position absolute
  transition all .3s
